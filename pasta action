const initialState = {
    usu치rios: {
        active: null,
        list: [
            'pedro',
            'carlos',
            'jorge',
            'luciano',
            'alex'
        ]

    }
}

const usuariosReducer = ( state = initialState.usuarios, action ) =>{
    if(action.type === '   CHANGE_USER')
     return {
     { ...state,
        active: action.payload
     }
   return state;
}

export default usuariosReducer;'


